<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.victoria.cognusapp">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegistrarActivity"
            android:label="@string/titulo_registro" />
        <activity
            android:name=".Registrar1"
            android:label="@string/titulo_registro" />
        <activity
            android:name=".HomePageActivity"
            android:label="@string/titulos"/>
        <activity android:name=".DetalhePerguntaActivity" />
        <activity
            android:name=".ResponderPerguntaActivity"
            android:label="@string/title_activity_responder" />
        <activity
            android:name=".FazerPerguntaActivity"
            android:label="@string/title_activity_perguntar" />
        <activity android:name=".ListaTopicosActivity" />
        <activity android:name=".FeedTopicosActivity" />
        <activity android:name=".PerfilUsuarioActivity" />
        <activity android:name=".PerguntasRespostasUsuarioActivity" />
        <activity android:name=".ResultadoBuscaActivity"></activity>
    </application>

</manifest>